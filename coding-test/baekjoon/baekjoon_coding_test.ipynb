    "def print_star(n):\n",
    "    if n == 1:\n",
    "        return ['*']\n",
    "    star = print_star(n//3)\n",
    "    star_list = []\n",
    "    for i in star:\n",
    "        star_list.append(i*3)\n",
    "    for i in star:\n",
    "        star_list.append(i+' '*(n//3)+i)\n",
    "    for i in star:\n",
    "        star_list.append(i*3)\n",
    "        \n",
    "    return star_list\n",
    "\n",
    "N = int(input())\n",
    "print('\\n'.join(print_star(N)))"
   ]
