   "source": [
    "# math.sqrt를 사용하면 시간 초과가 뜬다.\n",
    "# 그래서 **0.5를 사용하자!\n",
    "def solution_2(k, d):\n",
    "    answer = 0\n",
    "    for i in range(0, d+1, k):\n",
    "        y = int((d*d - i*i)**0.5)\n",
    "        print('I:', i)\n",
    "        print('y:',y)\n",
    "        print('y//k:',y//k)\n",
    "        answer += (y//k + 1)\n",
    "        print('answer:',answer)\n",
    "    return answer"
   ]
